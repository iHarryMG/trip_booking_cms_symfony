<!DOCTYPE html>
<html lang="en">
    <head>
        {% block meta %}
            <meta charset="UTF-8">
            <meta charset="utf-8">
            <meta http-equiv="X-UA-Compatible" content="IE=edge">
            <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
            <meta name="description" content="">
            <meta name="author" content="">
        {% endblock %}
        <title>{% block title %}Leisure Admin{% endblock %}</title>
        {% block stylesheets %}
            <!-- Custom fonts for this template -->
            <link href="/library/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
            <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">

            <!-- Custom styles for this template -->
            <link href="/css/sb-admin-2.min.css" rel="stylesheet">

            <!-- Custom styles for this page -->
            <!--link href="/library/bootstrap_datetimepicker/css/bootstrap.min.css" rel="stylesheet" media="screen"-->
            <link href="/library/vendor/datatables/dataTables.bootstrap4.min.css" rel="stylesheet">
            <link rel="stylesheet" href="/library/uikit/css/uikit.min.css">
            
            <link href="/library/bootstrap_datetimepicker/css/bootstrap-datetimepicker.min.css" rel="stylesheet"  media="screen">
            <link href="/library/bootstrap_combobox/css/bootstrap-combobox.css" rel="stylesheet">

            <style>
                .table{
                    font-size: 14px;
                    color: #676767;
                }
            </style>
        {% endblock %}
        <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
        <![endif]-->
    </head>
    <body id="page-top">
        <div id="wrapper">
            
            {% block sidebar %}{% endblock %}

            <!-- Content Wrapper -->
            <div id="content-wrapper" class="d-flex flex-column">
                <!-- Main Content -->
                <div id="content">
                {% block navbar %}{% endblock %}
                {% block body %}{% endblock %}
                
                </div>
                
                {% block footer %}{% endblock %}
            </div>
            

        </div>
        <!-- End of Content Wrapper -->
    

        <!-- Scroll to Top Button-->
        <a class="scroll-to-top rounded" href="#page-top">
          <i class="fas fa-angle-up"></i>
        </a>
        <!-- Logout Modal-->
        <div class="modal fade" id="logoutModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
          <div class="modal-dialog" role="document">
            <div class="modal-content">
              <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Ready to Leave?</h5>
                <button class="close" type="button" data-dismiss="modal" aria-label="Close">
                  <span aria-hidden="true">Ã—</span>
                </button>
              </div>
              <div class="modal-body">Select "Logout" below if you are ready to end your current session.</div>
              <div class="modal-footer">
                <button class="btn btn-secondary" type="button" data-dismiss="modal">Cancel</button>
                <a class="btn btn-primary" href="login.html">Logout</a>
              </div>
            </div>
          </div>
        </div>
        
        {% block javascripts %}
            <!-- Bootstrap core JavaScript-->
            <script src="/library/vendor/jquery/jquery.min.js"></script>
            <script src="/library/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

            <!-- Core plugin JavaScript-->
            <script src="/library/vendor/jquery-easing/jquery.easing.min.js"></script>

            <!-- Custom scripts for all pages-->
            <script src="/js/sb-admin-2.min.js"></script>

            <!-- Page level plugins -->
            <script src="/library/vendor/datatables/jquery.dataTables.min.js"></script>
            <script src="/library/vendor/datatables/dataTables.bootstrap4.min.js"></script>
            
            <script src="/library/uikit/js/uikit.min.js"></script>
            <script src="/library/uikit/js/uikit-icons.min.js"></script>
            
            <script src="/library/bootstrap_datetimepicker/js/bootstrap.min.js" type="text/javascript" ></script>
            <script src="/library/bootstrap_datetimepicker/js/bootstrap-datetimepicker.min.js"></script>
            <script src="/library/bootstrap_combobox/js/bootstrap-combobox.js"></script>

            <!-- Local JS -->
            <script src="/js/common.js"></script>
        {% endblock %}
        
    </body>
</html>
